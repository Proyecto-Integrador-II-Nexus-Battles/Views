<!-- Cartas -->
<div class="container_productos">
  <div class="list">
  <% for (var i = 0; i < datos.length; i++) { %>
  <div class="card" onclick="vistaDetallada()">
    <div class="imagen_carta">
      <img src=<%= datos[i].imagen %> class="personaje">
    </div>
    <div class="informacion_carta">
      <p><%= datos[i].nombre %></p>
      <p>Poder: <%= datos[i].poder %></p>
      <p>Vida: <%= datos[i].vida %></p>
      <p>Defensa: <%= datos[i].defensa %></p>
      <p>Ataque: <%= datos[i].ataque %></p>
      <p>Da√±o: <%= datos[i].dano %></p>
    </div>
  </div>
  <% } %>
</div>
</div>
<div class="paginas">
<ul class="listPage">
    <li class="active">1</li>
    <li>2</li>
    <li>3</li>
</ul>
</div>
</div>
</main>
<script src="/js/navbar.js"></script>
<script src="/js/vistaDetallada.js"></script>
<script src="/js/paginacion.js"></script>
</body>
</html>